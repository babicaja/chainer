{
    "name": "babicaja/chainer",
    "description": "Chain your actions and make complex work flows more readable",
    "type": "library",
    "license": "MIT",
  "keywords": [
    "chain",
    "link",
    "flow",
    "chain-of-responsibility"
  ],
    "authors": [
      {
        "name": "Dejan BabiÄ‡",
        "email": "babicaja@gmail.com"
      }
    ],
  "require": {
    "php": "^7.4"
  },
  "autoload": {
    "psr-4": {
      "Chainer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.4",
    "nunomaduro/phpinsights": "^1.14",
    "squizlabs/php_codesniffer": "^3.5",
    "phpmd/phpmd": "^2.9"
  },
  "scripts": {
    "phpcs": "phpcs --standard=PSR12",
    "phpmd": "phpmd codesize,controversial,design,naming,unusedcode"
  }
}
